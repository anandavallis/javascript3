<body>
    <script>
        function factorial(n) {
    if (n < 0) return;
    if (n === 0 || n === 1);
    let result = 1;

    for (let i = 2; i <= n; i++) {
        result *= i;
    }
    return result;
}
console.log(factorial(5)); 
    </script>
</body>
</html>
